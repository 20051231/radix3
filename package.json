  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/mocha": "latest",
    "chai": "latest",
    "eslint": "latest",
    "jiti": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "standard-version": "latest",
    "unbuild": "latest",
    "@types/chai": "latest"
  }
