{
  "name": "radix3",
  "version": "0.1.0",
  "repository": "unjs/radix3",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs"
  },
